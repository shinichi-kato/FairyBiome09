    "version": "0.2.0",
    "configurations": [
        {
            "name": "Gatsby develop",
            "type": "node",
            "request": "launch",
            "protocol": "inspector",
            "program": "${workspaceFolder}/node_modules/gatsby/dist/bin/gatsby",
            "args": [
                "develop"
            ],
            "stopOnEntry": false,
            "runtimeArgs": [
                "--nolazy"
            ],
            "sourceMaps": false,
            "command": "echo ${workspaceFolder}"
        },
        {
            "name": "Gatsby build",
            "type": "node",
            "request": "launch",
            "protocol": "inspector",
            "program": "${workspaceFolder}/node_modules/gatsby/dist/bin/gatsby",
            "args": [
                "build"
            ],
            "stopOnEntry": false,
            "runtimeArgs": [
                "--nolazy"
            ],
            "sourceMaps": false
        },
        {
            "name": "Launch Chrome",
            "type": "pwa-chrome",
            "request": "launch",
            "url": "http://localhost:8000",
            "webRoot": "${workspaceFolder}"
        }
    ]
}